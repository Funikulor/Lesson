#include <iostream>
#include <string>
using namespace std;
class Children
{
private:
    string name;
    string surname;
    int age;
public:
    Children(string name, string surname, int age)
    {
        this->name = name;
        this->surname = surname;
        this->age = age;
        cout << this->name << endl << this->surname << endl << this->age << endl;
    }
};
int main() {
    Children child = Children("Alex", "Margenal", 12);
    Children child2 = Children("Ema", "Nazist", 16);
    return 0;
}
